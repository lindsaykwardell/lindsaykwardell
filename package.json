    "gridsome-plugin-rss": "^1.2.0",
    "gridsome-plugin-sentry": "^1.1.0",
    "http-proxy-middleware": "^1.0.1",
    "moment": "^2.24.0",
    "pyloncss": "^0.1.1",
    "unfetch": "^4.1.0",
    "uuid": "^7.0.2",
    "vue-awesome": "^4.0.2",
    "vuetensils": "^0.5.2"
  },
