  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build-functions && gatsby build",
    "build:css": "postcss index.css -o src/components/layout.css",
    "watch:css": "postcss index.css -o src/components/layout.css -w",
    "develop": "gatsby develop",
    "functions": "netlify-lambda serve --config ./webpack.functions.js functions",
    "build-functions": "netlify-lambda build functions",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "concurrently \"npm run develop\" \"npm run functions\"",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  }
