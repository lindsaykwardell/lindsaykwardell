  "scripts": {
    "build": "gridsome build",
    "develop": "gridsome develop",
    "explore": "gridsome explore",
    "build:css": "postcss index.postcss -o src/assets/main.css",
    "watch:css": "postcss index.postcss -o src/assets/main.css -w",
    "functions": "netlify-lambda serve --config ./webpack.functions.js functions",
    "build-functions": "netlify-lambda build functions",
    "start": "concurrently \"yarn develop\" \"yarn functions\"",
